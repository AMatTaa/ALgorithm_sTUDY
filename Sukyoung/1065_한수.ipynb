    "#1065 한수\n",
    "\n",
    "n = int(input())\n",
    "if n>99:\n",
    "    cnt = 0\n",
    "    for i in range(110,n+1):\n",
    "        num = list(map(int,list(str(i))))\n",
    "        if (num[2]-num[1]) == (num[1]-num[0]):\n",
    "            cnt += 1\n",
    "    print(99 + cnt)\n",
    "else:\n",
    "    print(n)\n",
    "  "
